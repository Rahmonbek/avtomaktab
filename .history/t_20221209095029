$('.pages_stu').append(`<div class="pages_item first ${page==1?"dis":''}" onclick="${setPage(page-1):''}"><</div>
            <div class="pages_item ${page==1?"active":''}" onclick="${setPage(1):''}">1</div>
            <div class="pages_item ${page==2?"active":''}" onclick="${setPage(2):''}">2</div>
            <div class="pages_item ${page==3?"active":''}" onclick="${setPage(3):''}">3</div>
            <div class="pages_item ${page==4?"active":''}" onclick="${setPage(4):''}">4</div>
            <div class="pages_item ${page==5?"active":''}" onclick="${setPage(5):''}">5</div>
            <div class="pages_item dis">...</div>
            <div class="pages_item">${max_page}</div>
            <div class="pages_item last">></div>
            `)